@{
  Layout = "_Layout";
}

<h1>Salon 206</h1>

<h2>@Model["stylists"].GetFirstName() 's Clients</h2>

<ul>
  @foreach(var client in Model["clients"])
  {
    <li><a href="/stylists/@client.GetStylist()/@client.GetStylist()-details/client-list/@client.GetId()details">@client.GetLastName(), @client.GetFirstName()</a></li>
  }
</ul>

<a href="/">Home</a>
<a href="/stylists/stylists-all">All stylists</a>
